@model ResponseListViewModel

@Html.Partial("AddResponse", new ResponseViewModel{TicketId=Model.TicketId,UserId=3})
<div class="row pb-2">
    <h2 class="text-primary">@Model.Title</h2>
    <hr />
</div>
<table class="table table-striped table-bordered">

    <tbody>
        @foreach (var response in Model.Responses)
        {
            <tr>
                <td>
                    @response.TicketResponse
                </td>
                <td>
                    @Html.Partial("DeleteResponse", new ResponseViewModel{TicketId=Model.TicketId, UserId = 3,Id=response.Id})
                </td>
            </tr>
        }
    </tbody>
</table>